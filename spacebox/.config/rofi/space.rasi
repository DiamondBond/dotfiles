* {
  background-color: #d3d3e7;
  border-color: #b3b9be;
  text-color: #121212;
  spacing: 0;
  width: 768;
}

inputbar {
  border: 1px 1px 1px 1px;
  children: [prompt,entry];
}

prompt {
  padding: 4px;
  border: 0px 1px 0px 0px;
}

textbox {
  background-color: #e7e5eb;
  border: 1px 1px 1px 1px;
  border-color: #b3b9be;
  padding: 2px 4px;
}

entry {
  padding: 4px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  border: 1px 1px 1px 1px;
  padding: 4px;
}

element selected {
  background-color: #afafd7;
}
