<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="emacs">
			<action name="Execute">
				<execute>emacsclient -c -n -a 'emacs'</execute>
			</action>
		</item>
		<item label="term">
			<action name="Execute">
				<execute>xfce4-terminal</execute>
			</action>
		</item>
		<item label="web">
			<action name="Execute">
				<execute>firefox</execute>
			</action>
		</item>
		<item label="files">
			<action name="Execute">
				<execute>thunar</execute>
			</action>
		</item>
		<item label="notes">
			<action name="Execute">
				<execute>emacsclient -c -n -a 'emacs' -e '(notes-edit)'</execute>
			</action>
		</item>
		<separator/>
		<menu id="root-menu-63833" label="apps">
			<item label="xterm">
				<action name="Execute">
					<execute>xterm</execute>
				</action>
			</item>
			<item label="zathura">
				<action name="Execute">
					<execute>zathura</execute>
				</action>
			</item>
			<item label="office">
				<action name="Execute">
					<execute>libreoffice</execute>
				</action>
			</item>
			<separator/>
			<item label="xaos">
				<action name="Execute">
					<execute>xaos</execute>
				</action>
			</item>
			<item label="paint">
				<action name="Execute">
					<execute>kolourpaint</execute>
				</action>
			</item>
			<item label="spotify">
				<action name="Execute">
					<execute>env LD_PRELOAD=/usr/local/lib/spotify-adblock.so spotify</execute>
				</action>
			</item>
		<item label="music">
			<action name="Execute">
				<execute>xfce4-terminal --geometry 55x15 -e ncmpcpp</execute>
			</action>
		</item>
			<item label="mixer">
				<action name="Execute">
					<execute>pavucontrol</execute>
				</action>
			</item>
			<separator/>
			<item label="hexchat">
				<action name="Execute">
					<execute>hexchat</execute>
				</action>
			</item>
			<item label="telegram">
				<action name="Execute">
					<execute>telegram-desktop</execute>
				</action>
			</item>
			<item label="discord">
				<action name="Execute">
					<execute>discord</execute>
				</action>
			</item>
			<item label="transmission">
				<action name="Execute">
					<execute>transmission-gtk</execute>
				</action>
			</item>
		</menu>
		<menu id="settings" label="misc">
			<menu id="settings-99037" label="cpu">
				<item label="monitor">
					<action name="Execute">
						<execute>/home/diamond/bin/sensmon</execute>
					</action>
				</item>
				<item label="performance">
					<action name="Execute">
						<execute>xfce4-terminal -e &quot;/home/diamond/bin/maxperf&quot;</execute>
					</action>
				</item>
				<item label="powersave">
					<action name="Execute">
						<execute>xfce4-terminal -e &quot;/home/diamond/bin/minperf&quot;</execute>
					</action>
				</item>
			</menu>
			<menu id="settings-41155" label="session">
				<item label="info">
					<action name="Execute">
						<execute>xfce4-terminal --title &quot;System Information&quot; --geometry 36x6 -e &quot;/home/diamond/bin/sysinfo&quot;</execute>
					</action>
				</item>
				<item label="fetch">
					<action name="Execute">
						<execute>xfce4-terminal --title &quot;fetch&quot; --geometry 73x20 -e &quot;/home/diamond/bin/fetchinfo&quot;</execute>
					</action>
				</item>
			<separator/>
				<item label="reconfig">
					<action name="Reconfigure"/>
				</item>
				<item label="restart">
					<action name="Restart"/>
				</item>
			<separator/>
				<item label="logout">
					<action name="Exit"/>
				</item>
			</menu>
		</menu>
	</menu>
</openbox_menu>
